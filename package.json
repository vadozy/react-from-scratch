{
  "name": "react-from-scratch",
  "version": "1.0.0",
  "description": "Based on the medium.com, by @TeeFouad create-a-react-application-from-scratch",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "babel-node ./server/index.js",
    "start:prod": "NODE_ENV=production babel-node ./server/index.js",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "dev_old2": "nodemon --ignore lib/ --ignore public/ --exec 'env-cmd webpack --config ./webpack/config.babel.js; babel-node ./server/index.js'",
    "lint": "eslint . --ext .js,.jsx",
    "babel": "babel src -d lib",
    "build:client": "env-cmd webpack --config ./webpack/config.babel.js",
    "build": "NODE_ENV=production env-cmd webpack --config ./webpack/config.prod.babel.js",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Vadim Storozhuk",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-register": "^6.26.0",
    "css-loader": "^2.1.1",
    "env-cmd": "^9.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "webpack": "^4.32.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "pre-commit": [
    "lint"
  ]
}
